                </Assign.To>
                <Assign.Value>
                  <InArgument x:TypeArguments="x:Int32">0</InArgument>
                </Assign.Value>
              </Assign>
              <While sap2010:WorkflowViewState.IdRef="While_2" Condition="[Not isProcessedCorrectly and (processingTry &lt; 3)]">
