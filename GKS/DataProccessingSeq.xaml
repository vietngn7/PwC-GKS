                              <Sequence DisplayName="Выгружаем данные" sap2010:WorkflowViewState.IdRef="Sequence_2">
                                <Sequence.Variables>
                                  <Variable x:TypeArguments="sd:DataTable" Default="[new System.Data.DataTable()]" Name="ExtractedData" />
                                </Sequence.Variables>
                                <If Condition="[InStr(row(1).ToString, &quot;stat&quot;) &gt; 0]" sap2010:WorkflowViewState.IdRef="If_1">
                                  <If.Then>
                                    <ui:InvokeWorkflowFile ContinueOnError="{x:Null}" DisplayName="Вызывается шаг для выгрузки данных с fedstat" sap2010:WorkflowViewState.IdRef="InvokeWorkflowFile_1" UnSafe="False" WorkflowFileName="Fedstat.xaml">
                                      <ui:InvokeWorkflowFile.Arguments>
                                        <InOutArgument x:TypeArguments="sd:DataRow" x:Key="row">[row]</InOutArgument>
                                        <InArgument x:TypeArguments="ui:Browser" x:Key="browser">[rbaBrowser]</InArgument>
                                        <OutArgument x:TypeArguments="sd:DataTable" x:Key="ExtractDataTable">[ExtractedData]</OutArgument>
                                        <OutArgument x:TypeArguments="x:String" x:Key="statusLine">[currentStatus]</OutArgument>
                                      </ui:InvokeWorkflowFile.Arguments>
                                    </ui:InvokeWorkflowFile>
                                  </If.Then>
                                  <If.Else>
                                    <ui:InvokeWorkflowFile ContinueOnError="{x:Null}" DisplayName="Вызывается шаг для выгрузки данных с ГКС" sap2010:WorkflowViewState.IdRef="InvokeWorkflowFile_2" UnSafe="False" WorkflowFileName="GKS.xaml">
                                      <ui:InvokeWorkflowFile.Arguments>
                                        <InOutArgument x:TypeArguments="sd:DataRow" x:Key="row">[row]</InOutArgument>
                                        <InArgument x:TypeArguments="ui:Browser" x:Key="browser">[rbaBrowser]</InArgument>
                                        <OutArgument x:TypeArguments="sd:DataTable" x:Key="ExtractDataTable">[ExtractedData]</OutArgument>
                                        <OutArgument x:TypeArguments="x:String" x:Key="statusLine">[currentStatus]</OutArgument>
                                      </ui:InvokeWorkflowFile.Arguments>
                                    </ui:InvokeWorkflowFile>
                                  </If.Else>
                                </If>
                                <ui:InvokeWorkflowFile ContinueOnError="{x:Null}" DisplayName="Обновляем данные в таблице по результатам загрузки" sap2010:WorkflowViewState.IdRef="InvokeWorkflowFile_3" UnSafe="False" WorkflowFileName="StUpdateData.xaml">
                                  <ui:InvokeWorkflowFile.Arguments>
                                    <InArgument x:TypeArguments="sd:DataTable" x:Key="ExtractDataTable">[ExtractedData]</InArgument>
                                    <InArgument x:TypeArguments="x:String" x:Key="beginSearchData">[lastMonthYear]</InArgument>
                                    <InArgument x:TypeArguments="x:Int32" x:Key="beginIndex">[lastIndex]</InArgument>
                                    <InOutArgument x:TypeArguments="sd:DataTable" x:Key="originalDataTable">[dt]</InOutArgument>
                                    <InArgument x:TypeArguments="x:Int32" x:Key="currentRowIndex">[currentRowIndex]</InArgument>
                                    <OutArgument x:TypeArguments="x:String" x:Key="statusLine">[currentStatus]</OutArgument>
                                  </ui:InvokeWorkflowFile.Arguments>
                                </ui:InvokeWorkflowFile>
                                <Assign sap2010:WorkflowViewState.IdRef="Assign_6">
                                  <Assign.To>
                                    <OutArgument x:TypeArguments="x:Boolean">[isProcessedCorrectly]</OutArgument>
                                  </Assign.To>
                                  <Assign.Value>
                                    <InArgument x:TypeArguments="x:Boolean">True</InArgument>
                                  </Assign.Value>
                                </Assign>
                              </Sequence>
                            </TryCatch.Try>
                            <TryCatch.Catches>
                              <Catch x:TypeArguments="s:Exception" sap2010:WorkflowViewState.IdRef="Catch`1_1">
                                <ActivityAction x:TypeArguments="s:Exception">
                                  <ActivityAction.Argument>
                                    <DelegateInArgument x:TypeArguments="s:Exception" Name="exception" />
                                  </ActivityAction.Argument>
                                  <Sequence DisplayName="Обрабатываем ошибку выгрузки" sap2010:WorkflowViewState.IdRef="Sequence_5">
                                    <Assign>
                                      <Assign.To>
                                        <OutArgument x:TypeArguments="x:String">[currentStatus]</OutArgument>
                                      </Assign.To>
                                      <Assign.Value>
                                        <InArgument x:TypeArguments="x:String">["Для материала " &amp; row(0).ToString &amp; " не удалось выполнить выгрузку. Попытка " &amp; processingTry.ToString &amp; " из 3"]</InArgument>
                                      </Assign.Value>
                                      <sap2010:WorkflowViewState.IdRef>Assign_9</sap2010:WorkflowViewState.IdRef>
                                    </Assign>
                                    <ui:LogMessage DisplayName="Log Message" sap2010:WorkflowViewState.IdRef="LogMessage_1" Level="Error" Message="[currentStatus]" />
                                    <Assign sap2010:WorkflowViewState.IdRef="Assign_7">
                                      <Assign.To>
                                        <OutArgument x:TypeArguments="x:Boolean">[isProcessedCorrectly]</OutArgument>
                                      </Assign.To>
                                      <Assign.Value>
                                        <InArgument x:TypeArguments="x:Boolean">False</InArgument>
                                      </Assign.Value>
                                    </Assign>
                                    <ui:RefreshBrowser Browser="[rbaBrowser]" DisplayName="Refresh Browser" sap2010:WorkflowViewState.IdRef="RefreshBrowser_1" />
                                  </Sequence>
                                </ActivityAction>
                              </Catch>
                            </TryCatch.Catches>
                            <TryCatch.Finally>
                              <Assign sap2010:WorkflowViewState.IdRef="Assign_8">
                                <Assign.To>
                                  <OutArgument x:TypeArguments="x:Int32">[processingTry]</OutArgument>
                                </Assign.To>
                                <Assign.Value>
                                  <InArgument x:TypeArguments="x:Int32">[processingTry + 1]</InArgument>
                                </Assign.Value>
                              </Assign>
                            </TryCatch.Finally>
                          </TryCatch>
                        </Sequence>
                      </While>
                      <AddToCollection x:TypeArguments="x:String" Collection="[statusList]" DisplayName="Add To Collection" Item="[currentStatus]" sap2010:WorkflowViewState.IdRef="AddToCollection`1_2" />
                    </Sequence>
                  </ActivityAction>
                </ui:ForEachRow.Body>
              </ui:ForEachRow>
            </Sequence>
            <ui:CloseTab Browser="{x:Null}" DisplayName="Закрываем последнюю вкладку" sap2010:WorkflowViewState.IdRef="CloseTab_1" />
          </Sequence>
        </ActivityAction>
      </ui:OpenBrowser.Body>
